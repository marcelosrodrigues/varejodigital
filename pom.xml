                        <artifactId>jetty-util</artifactId>
                        <version>9.2.6.v20141205</version>
                    </dependency>
                </dependencies>
                <executions>
                    <execution>
                        <id>start</id>
                        <phase>pre-integration-test</phase>
                        <goals>
                            <goal>start</goal>
                        </goals>
                        <configuration>
                            <scanIntervalSeconds>0</scanIntervalSeconds>
                            <jettyXml>src/test/resources/jetty-env.xml</jettyXml>
                            <scanTargets>
                                <scanTarget>${basedir}/src/test/resources/akatus.properties</scanTarget>
                            </scanTargets>
                        </configuration>
                    </execution>
                    <execution>
                        <id>complete</id>
                        <phase>post-integration-test</phase>
                        <goals>
                            <goal>stop</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <phase>package</phase>
