                    </webApp>
                    <stopKey>foo</stopKey>
                    <stopPort>9999</stopPort>
                    <httpConnector>
                        <port>8080</port>
                        <name>org.mortbay.jetty.nio.SelectChannelConnector</name>
                    </httpConnector>
                    <jettyXml>src/test/resources/jetty-env.xml</jettyXml>
                    <scanTargets>
                        <scanTarget>${basedir}/src/test/resources/upload.properties</scanTarget>
                    </scanTargets>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>commons-dbcp</groupId>
                        <artifactId>commons-dbcp</artifactId>
                        <version>1.4</version>
                    </dependency>
                    <dependency>
