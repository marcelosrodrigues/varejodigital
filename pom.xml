                                    <artifactId>jetty-util</artifactId>
                                    <version>9.2.6.v20141205</version>
                                    <destFileName>jetty-util.jar</destFileName>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
